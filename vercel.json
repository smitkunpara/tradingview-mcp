{
  "version": 2,
  "builds": [
    {
      "src": "vercel/index.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.11",
        "installer": "uv",
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel/index.py"
    }
  ]
}